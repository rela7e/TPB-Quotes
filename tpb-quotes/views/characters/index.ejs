<%- include('../partials/header') %>

<thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
    </tr>
  </thead>
  <tbody>
    <% characters.forEach(function(c) { %>
      <tr>
        <td><%= c.name %></td>
        <td><%= c.description %></td>
        <td><a href="/characters/<%= m._id %>">DETAILS</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>


<%- include('../partials/footer') %>